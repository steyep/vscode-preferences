            "tab.activeBackground": "#19191f",
            // "statusBar.debuggingBackground": "#515c7e45",
            "statusBar.debuggingForeground": "#1abc9c"
        },
        "[Material Theme Ocean]": {
            "sideBar.border": "#483e4e",
            "sideBar.background": "#060611",
        },
        "[Material Theme Palenight]": {
            "sideBar.border": "#2e3244",
            "sideBar.background": "#242735",
        },
        "[Material Theme Ocean High Contrast]": {
            "sideBar.border": "#483e4e",
