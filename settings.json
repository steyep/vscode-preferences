{
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.tabSize": 2,
    "editor.lineHeight": 28,
    "editor.minimap.renderCharacters": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.scrollBeyondLastLine": false,
    "editor.occurrencesHighlight": false,
    "explorer.openEditors.visible": 0,
    "phpcs.standard": "Drupal,DrupalPractice",
    "php.suggest.basic": false,
    "php.executablePath": "/usr/local/opt/php70/bin/php",
    "[php]": {
        "editor.rulers": [80]
    },
    "php-docblocker.gap": true,
    "php-docblocker.useShortNames": true,
    "php-docblocker.qualifyClassNames": true,
    "workbench.tree.horizontalScrolling": true,
    "workbench.startupEditor": "none",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Droid Sans Mono for Powerline",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.5,
    "files.trimTrailingWhitespace": true,
    "extensions.ignoreRecommendations": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "gitlens.keymap": "alternate",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "keyword.control"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Material Theme Darker]": {
            "variables": "#ff5370"
        }
    },
    "workbench.colorCustomizations": {
        "scrollbar.shadow": "#151b1f65",
        "editor.findMatchHighlightBackground": "#16d0ff28",
    },
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": true,
    "window.newWindowDimensions": "inherit",
    "editor.parameterHints": false,
    "editor.emptySelectionClipboard": false,
    "window.openFilesInNewWindow": "off",
    "editor.wordWrap": "off",
    "zenMode.fullScreen": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.folders.color": "#42a5f5",
    "workbench.activityBar.visible": true,
    "better-phpunit.phpunitBinary": "phpunit",
    "launch": {
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [{
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9001,
            "xdebugSettings": {
                "show_hidden": 1
            }
        }]
    },
    "gitlens.historyExplorer.enabled": false,
    "files.associations": {
        "*.module": "php",
        "*.inc": "php",
        "*.install": "php",
        ".gitconfig.symlink": "shellscript"
    },
    "php-docblocker.returnGap": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.swipeToNavigate": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.colorTheme": "Enki - Rainbow Bro",
    "materialTheme.autoApplyIcons": false,
    "bookmarks.navigateThroughAllFiles": false,
    "workbench.activityBar.visible": true,
    "explorer.autoReveal": false,
    "breadcrumbs.enabled": true,
    "eslint.nodePath": "/usr/local/lib/node_modules/eslint-config-airbnb-bundle",
    "editor.fontSize": 15,
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-seriffont",
    "materialTheme.showReloadNotification": false,
    "window.zoomLevel": 0,
    "debug.internalConsoleOptions": "neverOpen",
    "debug.openDebug": "neverOpen",
    "debug.openExplorerOnEnd": true,
    "debug.showInStatusBar": "never",
    "macros": {
        "startDebug": [
            "workbench.view.debug",
            "workbench.action.debug.start"
        ]
    },
    "window.nativeFullScreen": false,
}