{
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.tabSize": 2,
    "editor.lineHeight": 26,
    "editor.minimap.renderCharacters": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.scrollBeyondLastLine": false,
    "editor.occurrencesHighlight": false,
    "explorer.openEditors.visible": 0,
    "phpcs.standard": "Drupal,DrupalPractice",
    "php.suggest.basic": false,
    "php.executablePath": "/usr/local/opt/php70/bin/php",
    "[php]": {
        "editor.rulers": [80]
    },
    "php-docblocker.gap": true,
    "php-docblocker.useShortNames": true,
    "workbench.tree.horizontalScrolling": true,
    "workbench.startupEditor": "none",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Droid Sans Mono for Powerline",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.5,
    "files.trimTrailingWhitespace": true,
    "extensions.ignoreRecommendations": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "workbench.colorTheme": "Material Theme Darker",
    "gitlens.keymap": "alternate",
    "workbench.iconTheme": "eq-material-theme-icons",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "keyword.control"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Material Theme Darker]": {
            "variables": "#ff5370"
        }
    },
    "workbench.colorCustomizations": {
        "scrollbar.shadow": "#151b1f65",
        "editor.findMatchHighlightBackground": "#16d0ff28",
    },
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": true,
    "window.newWindowDimensions": "inherit",
    "editor.parameterHints": false,
    "editor.emptySelectionClipboard": false,
    "window.openFilesInNewWindow": "off",
    "editor.wordWrap": "off",
    "zenMode.fullScreen": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.folders.color": "#42a5f5",
    "workbench.activityBar.visible": true,
    "better-phpunit.phpunitBinary": "phpunit",
    "launch": {
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Listen for XDebug",
                "type": "php",
                "request": "launch",
                "port": 9001,
                "serverSourceRoot": "${workspaceRoot}",
                "localSourceRoot": "${workspaceRoot}"
            },
            {
                "name": "Launch currently open script",
                "type": "php",
                "request": "launch",
                "program": "${file}",
                "cwd": "${fileDirname}",
                "port": 9001
            }
        ]
    }
}