{
    "editor.tabSize": 2,
    "editor.lineHeight": 30,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.scrollBeyondLastLine": false,
    "editor.occurrencesHighlight": false,
    "explorer.openEditors.visible": 0,
    "scm.defaultViewMode": "tree",
    "phpcs.standard": "Drupal,DrupalPractice",
    "phpcs.ignorePatterns": [
        "*/**/*.drushrc.php",
        "*/.local/*",
    ],
    "phpcs.trace.server": "verbose",
    "phpcs.showSources": true,
    "phpcs.autoConfigSearch": false,
    "editor.rulers": [80],
    "php.suggest.basic": false,
    "[php]": {
        "editor.rulers": [80]
    },
    "[csharp]": {
        "editor.tabSize": 4,
    },
    "php-docblocker.gap": true,
    "php-docblocker.useShortNames": true,
    "php-docblocker.qualifyClassNames": true,
    "workbench.startupEditor": "none",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Droid Sans Mono for Powerline",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.5,
    "files.trimTrailingWhitespace": true,
    "files.defaultLanguage": "${activeEditorLanguage}",
    "extensions.ignoreRecommendations": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
    },
    "gitlens.keymap": "alternate",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "keyword.control"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Community Material Theme Darker]": {
            "variables": "#ff5370"
        }
    },
    "workbench.colorCustomizations": {
        "[Tokyo Night]": {
            "scrollbar.shadow": "#151b1f65",
            "list.hoverBackground": "#1c1d29bb"
        }
    },
    "editor.snippetSuggestions": "top",
    "window.newWindowDimensions": "inherit",
    "editor.emptySelectionClipboard": false,
    "window.openFilesInNewWindow": "off",
    "editor.wordWrap": "off",
    "zenMode.fullScreen": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?-",
    "launch": {
        "version": "0.2.0",
        "configurations": [{
            "name": "Listen for XDebug (DDEV)",
            "type": "php",
            "request": "launch",
            "port": 9003,
            "pathMappings": {
                "/var/www/html": "${workspaceFolder}"
            },
            "log": false,
            "xdebugSettings": {
                "show_hidden": 1
            }
        }]
    },
    "files.associations": {
        "*.module": "php",
        "*.inc": "php",
        "*.install": "php",
        "*.symlink": "shellscript",
        "*.j2": "twig",
        "*.theme": "php",
        "*.profile": "php",
        "*.engine": "php"
    },
    "php.validate.executablePath": "/usr/local/bin/php",
    "php-docblocker.returnGap": true,
    "bookmarks.navigateThroughAllFiles": false,
    "explorer.autoReveal": false,
    "breadcrumbs.enabled": true,
    "editor.fontSize": 16,
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-seriffont",
    "debug.internalConsoleOptions": "neverOpen",
    "debug.openDebug": "neverOpen",
    "debug.openExplorerOnEnd": true,
    "debug.showInStatusBar": "never",
    "macros": {
        "startDebug": [
            "workbench.view.debug",
            "workbench.action.debug.start"
        ]
    },
    "intelephense.trace.server": "messages",
    "workbench.list.horizontalScrolling": true,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Dank Mono', Menlo, Monaco, 'Courier New', monospace",
    "workbench.tree.indent": 20,
    "rainbow_csv.autodetect_separators": [
        "\t",
        ",",
        ";",
        "|"
    ],
    "extensions.autoUpdate": false,
    "search.exclude": {
        "**/.git": true,
        "**/modules/contrib": false
    },
    "liveshare.increasedGuestLimit": false,
    "liveshare.populateGitCoAuthors": "never",
    "liveshare.showInStatusBar": "whileCollaborating",
    "workbench.iconTheme": "eq-material-theme-icons-palenight",
    "workbench.panel.defaultLocation": "right",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[xml]": {
        "editor.defaultFormatter": "mblode.pretty-formatter"
    },
    "explorer.confirmDragAndDrop": false,
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "search.useIgnoreFiles": false,
    "diffEditor.codeLens": true,
    "editor.renderWhitespace": "none",
    "workbench.editor.mouseBackForwardToNavigate": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "twig": "html"
    },
    "workbench.editor.untitled.hint": "hidden",
    "redhat.telemetry.enabled": false,
    "liveshare.presence": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.gpuAcceleration": "off",
    "files.exclude": {
        "**/.git": false,
    },
    "workbench.tree.renderIndentGuides": "none",
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.tabSizing": "shrink",
    "editor.minimap.renderCharacters": false,
    "scm.inputFontFamily": "editor",
    "scm.inputFontSize": 15,
    "advancedNewFile.convenienceOptions": [
        "last",
        "current",
        "root"
    ],
    "advancedNewFile.exclude": {
        "node_modules": true,
        "vendor": true,
        "dist": true
    },
    "window.commandCenter": true,
    "workbench.preferredDarkColorTheme": "Tokyo Night",
    "workbench.preferredLightColorTheme": "Tokyo Night Light",
    "thunder-client.htmlView": "Preview",
    "workbench.colorTheme": "Tokyo Night",
    "outline.showVariables": false,
    "thunder-client.saveRequestOnSend": false,
    "gitlens.views.contributors.showStatistics": true,
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": false,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "editor.guides.bracketPairsHorizontal": false,
    "atlascode.jira.enabled": true,
    "atlascode.bitbucket.explorer.relatedBitbucketIssues.enabled": false,
    "atlascode.bitbucket.explorer.notifications.pullRequestCreated": false,
    "atlascode.bitbucket.issues.explorerEnabled": false,
    "atlascode.bitbucket.contextMenus.enabled": true,
    "atlascode.bitbucket.statusbar.enabled": false,
    "atlascode.bitbucket.enabled": true,
    "atlascode.jira.statusbar.showUser": false,
    "atlascode.jira.statusbar.showProduct": false,
    "atlascode.jira.statusbar.enabled": true,
    "atlascode.jira.statusbar.showLogin": true,
    "atlascode.jira.todoIssues.triggers": [
        "@todo",
        "BUG:",
        "FIXME:",
        "ISSUE:"
    ],
    "atlascode.offlineMode": true,
    "atlascode.helpExplorerEnabled": false,
    "atlascode.jira.startWorkBranchTemplate.customPrefixes": [
        "feature",
        "bugfix",
        "release"
    ],
    "atlascode.bitbucket.issues.monitorEnabled": false,
    "atlascode.bitbucket.pipelines.branchFilters": [
        "develop"
    ],
    "atlascode.bitbucket.statusbar.showProduct": false,
    "atlascode.bitbucket.explorer.nestFilesEnabled": false,
    "atlascode.bitbucket.pipelines.refreshInterval": 15,
    "atlascode.bitbucket.preferredRemotes": [
        "origin"
    ],
    "atlascode.bitbucket.statusbar.showUser": false,
    "atlascode.jira.showCreateIssueProblems": true,
    "atlascode.jira.startWorkBranchTemplate.customTemplate": "{{{prefix}}}/{{{issueKey}}}-{{{summary}}}",
}