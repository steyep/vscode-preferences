    "gitlens.keymap": "alternate",
    "editor.tokenColorCustomizations": {
        "[Ste Enki]": {
            "textMateRules": [
                {
                    "scope": [
                        "storage.type",
                    ],
                    "settings": {
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": ["keyword.other.DML.sql"],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Ste Enki]": {
            // "sideBar.background": "#19191f",
            "editorGroupHeader.tabsBackground": "#19191f",
