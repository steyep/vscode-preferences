    "editor.fontSize": 16,
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-seriffont",
    "materialTheme.showReloadNotification": false,
    "window.zoomLevel": 0,
    "debug.internalConsoleOptions": "neverOpen",
