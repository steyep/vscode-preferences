{
    "editor.tabSize": 2,
    "editor.lineHeight": 30,
    "editor.minimap.renderCharacters": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.scrollBeyondLastLine": false,
    "editor.occurrencesHighlight": false,
    "explorer.openEditors.visible": 0,
    "phpcs.standard": "Drupal,DrupalPractice",
    "phpcs.ignorePatterns": [
        "*/**/*.drushrc.php",
        "*/.local/*",
    ],
    "phpcs.trace.server": "verbose",
    "phpcs.showSources": true,
    "phpcs.autoConfigSearch": false,
    "php.suggest.basic": false,
    "[php]": {
        "editor.rulers": [80]
    },
    "[csharp]": {
        "editor.tabSize": 4,
    },
    "php-docblocker.gap": true,
    "php-docblocker.useShortNames": true,
    "php-docblocker.qualifyClassNames": true,
    "workbench.startupEditor": "none",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Droid Sans Mono for Powerline",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.5,
    "files.trimTrailingWhitespace": true,
    "extensions.ignoreRecommendations": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "gitlens.keymap": "alternate",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "keyword.control"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Material Theme Darker]": {
            "variables": "#ff5370"
        }
    },
    "workbench.colorCustomizations": {
        "scrollbar.shadow": "#151b1f65",
    },
    "editor.snippetSuggestions": "top",
    "window.newWindowDimensions": "inherit",
    "editor.parameterHints": false,
    "editor.emptySelectionClipboard": false,
    "window.openFilesInNewWindow": "off",
    "editor.wordWrap": "off",
    "zenMode.fullScreen": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?-",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.folders.color": "#42a5f5",
    "workbench.activityBar.visible": true,
    "better-phpunit.phpunitBinary": "phpunit",
    "launch": {
        // Use IntelliSense to learn about possible attributes.
        // Hover to view descriptions of existing attributes.
        // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
        "version": "0.2.0",
        "configurations": [{
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9001,
            "xdebugSettings": {
                "show_hidden": 1
            }
        }]
    },
    "gitlens.historyExplorer.enabled": false,
    "files.associations": {
        "*.module": "php",
        "*.inc": "php",
        "*.install": "php",
        "*.symlink": "shellscript",
        "*.j2": "twig",
        "*.theme": "php",
    },
    "php-docblocker.returnGap": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.colorTheme": "Enki - Rainbow Bro",
    "materialTheme.autoApplyIcons": false,
    "bookmarks.navigateThroughAllFiles": false,
    "explorer.autoReveal": false,
    "breadcrumbs.enabled": true,
    "editor.fontSize": 16,
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'HelveticaNeue-Light', 'Ubuntu', 'Droid Sans', sans-seriffont",
    "materialTheme.showReloadNotification": false,
    "window.zoomLevel": 0,
    "debug.internalConsoleOptions": "neverOpen",
    "debug.openDebug": "neverOpen",
    "debug.openExplorerOnEnd": true,
    "debug.showInStatusBar": "never",
    "macros": {
        "startDebug": [
            "workbench.view.debug",
            "workbench.action.debug.start"
        ]
    },
    "gitlens.views.fileHistory.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    "intelephense.trace.server": "messages",
    "intelephense.debug.enable": false,
    "workbench.useExperimentalGridLayout": true,
    "workbench.list.horizontalScrolling": true,
    "editor.fontLigatures": true,
    "colorize.languages": [
        "json",
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg"
    ],
    "editor.fontFamily": "'Dank Mono', Menlo, Monaco, 'Courier New', monospace",
    "workbench.tree.indent": 15,
    "rainbow_csv.autodetect_separators": [
        "\t",
        ",",
        ";",
        "|"
    ],
    "extensions.autoUpdate": false,
    "search.exclude": {
        "config": true,
        "**/modules/contrib": false,
    },
}